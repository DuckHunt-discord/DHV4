"Commands to edit permissions.\n"
"\n"
"BANNED = 0\n"
"DEFAULT = 50\n"
"TRUSTED = 100\n"
"MODERATOR = 200\n"
"ADMIN = 300\n"
"BOT_MODERATOR = 500\n"
"BOT_OWNER = 600"
